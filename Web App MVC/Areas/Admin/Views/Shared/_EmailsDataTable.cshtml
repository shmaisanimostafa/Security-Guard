<!-- Main content -->
<section class="content">
    <div class="container-fluid">
        <div class="row">
            <div class="col-12">
                <!-- /.card-header -->
                <div class="card">
                    <div class="card-header">
                        <h3 class="card-title">Table of Links</h3>
                    </div>
                    <!-- /.card-header -->
                    <div class="card-body">
                        <table id="example2" class="table table-bordered table-striped">
                            <thead>
                                <tr>
                                    <th>Id</th>
                                    <th>UserName</th>
                                    <th>DateTime</th>
                                    <th>PredictedClass</th>
                                    <th>EmailMessage</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>

                                @{
                                    foreach (var Email in Model.PhishingEmails)

                                    {
                                        <tr>
                                            <td>@Email.Id</td>
                                            <td>@Email.UserName</td>
                                            <td>@Email.DateTime</td>
                                            <td>@Email.PredictedClass</td>
                                            <td>@Email.EmailMessage</td>
                                            <td>
                                                <form method="post" asp-controller="AdminDashboard" asp-action="DeleteEmail" asp-route-emailId="@Email.Id">
                                                    <button type="submit" class="btn btn-danger">
                                                        Delete Email
                                                    </button>
                                                </form>
                                            </td>
                                        </tr>
                                    }
                                }
                            </tbody>
                            <tfoot>
                                <tr>
                                    <th>Id</th>
                                    <th>UserName</th>
                                    <th>DateTime</th>
                                    <th>PredictedClass</th>
                                    <th>EmailMessage</th>
                                    <th>Action</th>
                                </tr>
                            </tfoot>
                        </table>
                    </div>
                    <!-- /.card-body -->
                </div>
                <!-- /.card -->
            </div>
            <!-- /.col -->
        </div>
        <!-- /.row -->
    </div>
    <!-- /.container-fluid -->
</section>